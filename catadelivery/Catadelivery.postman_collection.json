            "key": "Authorization",
            "value": "Bearer {{access_token}}"
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/users/{{user_id}}/",
          "host": ["{{base_url}}"],
          "path": ["api", "users", "{{user_id}}", ""]
        }
      }
    },
    {
      "name": "Role Change Request - Create",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{access_token}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"requested_role\": \"rider\",\n  \"message\": \"I would like to deliver orders in my area.\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/role-change-requests/"
        }
      }
    },
    {
      "name": "Products - List by Store",
      "request": {
        "method": "GET",
