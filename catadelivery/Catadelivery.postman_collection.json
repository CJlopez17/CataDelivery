          }
        ],
        "url": {
          "raw": "{{base_url}}/api/orders/"
        }
      }
    },
    {
      "name": "Products - Create",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{access_token}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Sample product\",\n  \"description\": \"Description\",\n  \"price\": 10.5,\n  \"category\": 1,\n  \"store\": 1\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/products/"
        }
      }
    },
    {
